[
  {
    "context": "Workspace",
    "bindings": {
      "ctrl-n": "workspace::ToggleLeftDock",
      "alt-j": "workspace::ActivateNextPane",
      "alt-k": "workspace::ActivatePreviousPane"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "space j": "editor::MoveDown",
      "space m": "editor::MoveUp"
    }
  },
  {
    "context": "Pane",
    "bindings": {
      "alt-n": "pane::ActivateNextItem",
      "alt-m": "pane::ActivatePrevItem",
      "ctrl-k w": "pane::SplitUp",
      "ctrl-k s": "pane::SplitDown",
      "ctrl-k a": "pane::SplitLeft",
      "ctrl-k d": "pane::SplitRight"
    }
  }
]
